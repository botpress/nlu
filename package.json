{
  "name": "@botpress/nlu",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NATIVE_EXTENSIONS_DIR=./native-extensions ts-node-dev ./packages/nlu/",
    "build": "cross-env NATIVE_EXTENSIONS_DIR=./native-extensions tsc --build ./packages/nlu",
    "start": "cross-env NATIVE_EXTENSIONS_DIR=./native-extensions node ./packages/nlu/dist/index.js",
    "pretty": "prettier --check './packages/**/*.ts' '!**/*.d.ts'",
    "eslint": "eslint packages/ --ext .ts",
    "e2e": "node ./packages/regression"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ]
}
